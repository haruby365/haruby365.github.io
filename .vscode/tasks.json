{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Webgen-Generate",
            "type": "shell",
            "command": "../Webgen/publish/HARUBY Webgen/HarubyCom.exe",
            "args": [
                "-s",
                "${workspaceFolder}/Source",
                "-t",
                "${workspaceFolder}/public",
                "-z",
                "${workspaceFolder}/../Webgen/HarubyCom"
            ],
            "problemMatcher": [],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "Webgen-Server",
            "type": "shell",
            "command": "../Webgen/publish/HARUBY Webgen/Server.exe",
            "args": [
                "${workspaceFolder}/public"
            ],
            "problemMatcher": []
        }
    ]
}